<?xml version="1.0"?>
<extension key="uk.co.compucorp.membershipextras" type="module">
  <file>membershipextras</file>
  <name>uk.co.compucorp.membershipextras</name>
  <description>This extension includes a huge amount of untility features that completes CiviCRM's membership management process. Notebly membership payment in instalments, payment plan management and more.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compuco</author>
    <email>hello@compuco.io</email>
  </maintainer>
  <releaseDate>2023-02-17</releaseDate>
  <version>16.2.0-02</version>
  <develStage>forked</develStage>
  <compatibility>
    <ver>5.79</ver>
  </compatibility>
  <comments>
    Use a reverse logic for a installment period
  </comments>
  <civix>
    <namespace>CRM/MembershipExtras</namespace>
    <format>24.09.1</format>
  </civix>
  <urls>
    <url desc="Documentation">https://github.com/compucorp/uk.co.compucorp.membershipextras/blob/master/readme.md</url>
  </urls>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CiviMix\Schema\Membershipextras\AutomaticUpgrader</upgrader>
</extension>
